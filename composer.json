{
  "name": "jojo1981/data-resolver",
  "description": "Generic extensible data resolver",
  "keywords": [
    "data",
    "resolver",
    "query",
    "tree",
    "predicate"
  ],
  "type": "library",
  "license": "MIT",
  "readme": "README.md",
  "authors": [
    {
      "name": "Joost Nijhuis",
      "email": "jnijhuis81@gmail.com"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "sebastian/comparator": "^3.0"
  },
  "require-dev": {
    "ext-json": "*",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpunit": "^7.5"
  },
  "suggest": {
    "jojo1981/data-resolver-handlers": "Add support to work with 3th party libraries"
  },
  "autoload": {
    "psr-4": {
      "Jojo1981\\DataResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\Jojo1981\\DataResolver\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  }
}
