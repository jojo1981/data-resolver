  "description": "Generic extensible data resolver",
  "keywords": [
    "data",
    "resolver",
    "query",
    "tree",
    "predicate"
  ],
  "type": "library",
  "license": "MIT",
  "readme": "README.md",
  "authors": [
    {
      "name": "Joost Nijhuis",
      "email": "jnijhuis81@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "phpunit/phpunit": "^7.5"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "2.1.x-dev"
  },
  "suggest": {
    "jojo1981/data-resolver-handlers": "Add support to work with 3th party libraries"
  },
  "autoload": {
    "psr-4": {
      "Jojo1981\\DataResolver\\": "src/"
    }
